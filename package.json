{
  "name": "jsreport-tags",
  "version": "2.4.0",
  "description": "jsreport extension adding organizational capabilities with tags",
  "main": "index.js",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 4300",
    "test": "mocha test --timeout 5000 && standard",
    "test:watch": "mocha test --watch",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-tags",
  "repository": {
    "type": "git",
    "url": "git+https://bjrmatos@github.com/jsreport/jsreport-tags.git"
  },
  "bugs": {
    "url": "https://github.com/jsreport/jsreport-tags/issues"
  },
  "license": "MIT",
  "keywords": [
    "jsreport",
    "tags"
  ],
  "author": "BJR Matos <bjrmatos@gmail.com> (https://github.com/bjrmatos)",
  "contributors": [
    "BJR Matos <bjrmatos@gmail.com> (https://github.com/bjrmatos)",
    "Jan Blaha"
  ],
  "files": [
    "lib",
    "shared",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "dependencies": {
    "lodash.debounce": "4.0.8",
    "mitt": "1.1.3"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.17.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-core": "2.9.0",
    "jsreport-studio-dev": "1.5.0",
    "mocha": "5.2.0",
    "should": "13.2.3",
    "standard": "10.0.3"
  },
  "engines": {
    "node": ">=8.9"
  }
}
